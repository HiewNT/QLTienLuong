# Exclude files from export
.gitattributes export-ignore
.gitignore export-ignore
.gitkeep export-ignore

# Exclude documentation from export (optional)
README.md export-ignore
LICENSE export-ignore
*.md export-ignore

# Exclude development files from export
*.Development.json export-ignore
*.Staging.json export-ignore
appsettings.Development.json export-ignore
appsettings.Staging.json export-ignore

# Exclude test files from export
**/Tests/ export-ignore
**/Test/ export-ignore
**/*Tests/ export-ignore
**/*Test/ export-ignore
*.Tests.csproj export-ignore
*.Test.csproj export-ignore

# Exclude CI/CD files from export (optional)
.github/ export-ignore
